		<property>controls/flols/distance-m</property>
	  	<x-offset>2.0</x-offset>
	  	<y-offset>2.0</y-offset>
	  	<z-offset>2.0</z-offset>
		<x-factor>0.005</x-factor>
		<y-factor>0.005</y-factor>
		<z-factor>0.005</z-factor>
    	</animation>


</PropertyList>
