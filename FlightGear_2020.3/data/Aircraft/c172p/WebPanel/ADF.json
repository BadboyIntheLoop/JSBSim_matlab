{
    "src":    "ADF.svg",

    "animations": [
      {
        "element": "#Card",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "a": {
            "property": "adf-rotation-deg",
            "scale": -1
          },
          "x": 256,
          "y": 256
        }]
      },{
        "element": "#Needle",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "a": {
            "property": "adf",
            "scale": 1
          },
          "x": 256,
          "y": 256
        }]
      }
    ]
}
