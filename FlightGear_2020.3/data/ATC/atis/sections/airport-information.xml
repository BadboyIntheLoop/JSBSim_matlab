<PropertyList>
  <token>airport-name</token>
  <if>
    <not-contains>
      <token>airport-name</token>
      <text>airport</text>
    </not-contains>
    <then>
      <text> airport</text>
    </then>
  </if>
  <text> information </text>
  <token>id</token>
  <text>. </text>
</PropertyList>
