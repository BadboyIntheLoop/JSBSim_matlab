<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

 <path>Saint_Mathieu_Signal_Tower.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>16000</max-m>
 </animation>
 
 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>Radar</object-name>
  <property>/sim/time/elapsed-sec</property>
  <factor>90.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>13.77</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

<model>
  <path>Models/Fauna/SeaGull_Origin.xml</path>
  <offsets>
  <x-m> 0.0</x-m>
  <y-m> 00.0</y-m>
  <z-m> 15.0</z-m>
  </offsets>
</model>

<model>
  <path>Models/Fauna/SeaGull_Origin_2.xml</path>
  <offsets>
  <x-m> 0.0</x-m>
  <y-m> 00.0</y-m>
