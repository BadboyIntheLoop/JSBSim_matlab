<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>
	<path>wasserturm_kornwestheim.ac</path>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>25000</max-m>
	</animation>

<animation>
    <type>material</type>
    <object-name>fenster</object-name>
    <condition>
        <greater-than>
          <property>/sim/time/sun-angle-rad</property>
          <value>1.47</value>
        </greater-than>
    </condition>
    <emission>	
	<red>0.45</red>	
	<green>0.4</green>	
	<blue>0.25</blue>
    </emission>
</animation>

</PropertyList>
