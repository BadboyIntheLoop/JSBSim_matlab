<animation>
  <type>rotate</type>
  <property>/environment/wind-from-heading-deg</property>
  <offset-deg>-90</offset-deg>
  <factor>-1</factor>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>Blades</object-name>
  <use-personality type="bool">true</use-personality>
  <property>/environment/wind-speed-kt</property>
  <interpolation>
	<entry>
      <ind>7.3</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>7.8</ind>
      <dep>9</dep>
    </entry>
    <entry>
      <ind>48.6</ind>
      <dep>19</dep>
    </entry>
	<entry>
      <ind>49.1</ind>
      <dep>0</dep>
    </entry>
  </interpolation>
  <starting-pos-deg>
   <random>
    <min>0</min>
    <max>360</max>
   </random>
  </starting-pos-deg>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>84.0</z-m>
  </center>
 </animation>

