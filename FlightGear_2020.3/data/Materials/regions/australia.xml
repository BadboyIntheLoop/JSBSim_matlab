<?xml version="1.0"?>
<!-- Regional definitions for Australia
Default Australia is modelled as dry.
Sand and soil is a reddish colour. Regional areas override
these definitions.
 -->

<PropertyList>
  <name>Australia</name>
  <area>
    <lon1>112.0</lon1>
    <lon2>155.0</lon2>
    <lat1>-45.0</lat1>
    <lat2>-14.0</lat2>
  </area>

  <area>
    <lon1>128.0</lon1>
    <lon2>146.0</lon2>
    <lat1>-14.0</lat1>
    <lat2>-10.0</lat2>
  </area>

  <area>
    <lon1>112.0</lon1>
    <lon2>128.0</lon2>
