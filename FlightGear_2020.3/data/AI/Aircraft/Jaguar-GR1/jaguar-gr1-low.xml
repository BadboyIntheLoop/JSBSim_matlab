<PropertyList>

  <path>jaguar-gr1-low.ac</path>

  <!-- Rentré des roues Temporairement disparition/apparition des roues -->
  <animation>
    <type>select</type>
    <object-name>axeAH</object-name>
    <object-name>roueA</object-name>
    <object-name>axeGH</object-name>
    <object-name>axeGB</object-name>
    <object-name>axeGBc</object-name>
    <object-name>roueG</object-name>
    <object-name>axeDH</object-name>
    <object-name>axeDB</object-name>
    <object-name>axeDBc</object-name>
    <object-name>roueD</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear[0]/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>
  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>20</factor>
    <axis>
      <x1-m>  7.033 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m>  0.488 </z1-m>
      <x2-m>  7.515 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m>  1.899 </z2-m>
    </axis>
  </animation>
  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeurG</object-name>
    <expression>
      <sum>
        <product>
          <property>surface-positions/right-aileron-pos-norm</property>
          <value>-6</value>
        </product>
        <product>
          <property>surface-positions/elevator-pos-norm</property>
          <value>-25</value>
        </product>
      </sum>
    </expression>
    <axis>
      <x1-m>  6.433 </x1-m>
      <y1-m> -0.254 </y1-m>
      <z1-m>  0.074 </z1-m>
      <x2-m>  7.548 </x2-m>
      <y2-m> -2.253 </y2-m>
      <z2-m> -0.380 </z2-m>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>profondeurD</object-name>
    <expression>
      <sum>
        <product>
          <property>surface-positions/right-aileron-pos-norm</property>
          <value>6</value>
        </product>
        <product>
          <property>surface-positions/elevator-pos-norm</property>
          <value>-25</value>
        </product>
      </sum>
    </expression>
    <axis>
      <x1-m>  7.548 </x1-m>
      <y1-m>  2.253 </y1-m>
      <z1-m> -0.380 </z1-m>
      <x2-m>  6.433 </x2-m>
      <y2-m>  0.254 </y2-m>
      <z2-m>  0.074 </z2-m>
    </axis>
  </animation>
  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <expression>
      <product>
        <max>
          <product>
            <property>surface-positions/left-aileron-pos-norm</property>
            <value>-1</value>
          </product>
          <value>0</value>
        </max>
        <value>45</value>
      </product>
    </expression>
    <axis>
      <x1-m>  3.075 </x1-m>
      <y1-m> -2.104 </y1-m>
      <z1-m> -0.041 </z1-m>
      <x2-m>  3.696 </x2-m>
